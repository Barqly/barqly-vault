<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Required for PyInstaller to load runtime-generated code (FFI, ctypes) -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>

    <!-- Required for PyInstaller to load Python extensions from temp directories -->
    <!-- Disables Library Validation check that all loaded libraries match Team ID -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
</dict>
</plist>
